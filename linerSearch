#include <iostream>
using namespace std;

int search(int A[], n, int key) 
{
  for ( int i = 0; i < n; i++) {
      if ( A[i] == key ) 
         return i;
  }
  return -1;
}

int main()
{
  int A[] = {2, 4, 6, 8, 10};
  int x;
  cout <<"Enter a key ";
  cin >> x ;
  int index = search(A, 5, x);
  if ( index > 0 ) {
    cout << "Element is found at the index " << index << endl;
  }
  return 0;
}
